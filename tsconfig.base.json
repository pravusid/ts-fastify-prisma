{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "skipLibCheck": true,
    "incremental": true,
    "declaration": true,
    "sourceMap": true,
    "removeComments": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "noImplicitOverride": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  }
}
